Package: modelr
Title: Environmental Niche Modeling Workflow Based on dismo
Version: 0.0.0.9000
Authors@R: c(
    person("Andrea", "SÃ¡nchez-Tapia", email = "andreasancheztapia@gmail.com", role = c("aut")),
    person("Felipe", "Barros", email = "felipe.b4rros@gmail.com", role = c("aut")),
